importFrom(stats, aggregate)
importFrom(graphics, boxplot, image)

export(as.mefa,
    as.stcs,
    cdrop,
    fill.na,
    is.dist,
    is.mefa,
    is.stcs,
    label,
    "label<-",
    mat2list,
